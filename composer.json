{
    "name": "progminer/php-datagen",
    "description": "Code generator for making data classes on PHP",
    "keywords": ["data", "class", "generator"],
    "license": "MIT",
    "authors": [
        {
            "name": "ProgMiner",
            "email": "eridan200@mail.ru"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "progminer/php-datagen-lib": "^0.1@alpha",
        "symfony/console": "^4.0",
        "symfony/finder": "^4.1"
    },
    "autoload": {
        "psr-4": {
            "PHPDataGen\\": "app"
        },
        "files": [
            "app/Utility/normalizePath.php"
        ]
    },
    "config": {
        "optimize-autoloader": true,
        "sort-packages": true
    },
    "bin": ["php-datagen"]
}
